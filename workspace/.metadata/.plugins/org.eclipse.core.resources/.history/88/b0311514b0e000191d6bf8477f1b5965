<?xml version="1.0" encoding="UTF-8" standalone="no"?><xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:firstElementOfTheInput="rss" xmlns:notXSLTCompatible="false" xmlns:own="http://ownfunction.org" xmlns:xs="http://www.w3.org/2001/XMLSchema" extension-element-prefixes="xs own notXSLTCompatible firstElementOfTheInput" version="2.0"><xsl:function name="own:setPrecision"><xsl:param name="resultString"/><xsl:param name="noOfDigits"/><xsl:if test="$noOfDigits=0"><xsl:value-of select="$resultString"/></xsl:if><xsl:if test="$noOfDigits!=0"><xsl:value-of select="own:setPrecision(concat($resultString,'0'),$noOfDigits - 1)"/></xsl:if></xsl:function><xsl:template match="/"><rss><channel><title><xsl:value-of select="rss/channel/title"/></title><link><xsl:value-of select="rss/channel/link"/></link><description><xsl:value-of select="rss/channel/description"/></description><image><title><xsl:value-of select="rss/channel/image/title"/></title><url><xsl:value-of select="rss/channel/image/url"/></url><link><xsl:value-of select="rss/channel/image/link"/></link></image><xsl:for-each select="rss/channel/item"><item><title><xsl:value-of select="title"/></title><link><xsl:value-of select="link"/></link><description><xsl:value-of select="description"/></description><pubDate><xsl:value-of select="pubDate"/></pubDate><guid><xsl:value-of select="guid"/></guid><dc:date><xsl:value-of select="dc:date"/></dc:date></item></xsl:for-each></channel></rss></xsl:template></xsl:stylesheet>